<h2>接口列表</h2>

<table class="table table-striped">
  <tr>
    <th>
    	接口中文名称
    </th>
    <th>
    	接口英文名称
    </th>
    <th>备注</th>
    <th>接口类型</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @interfaces.each do |interface| %>
  <tr>
    <td><%= interface.name_cn %></td>
    <td><%= interface.name_en %></td>
    <td><%= interface.remark %></td>
    <td>http</td>
    <td><%= link_to 'Show', interface %></td>
    <td><%= link_to 'Edit', edit_interface_path(interface) %></td>
    <td><%= link_to 'Destroy', interface, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />
